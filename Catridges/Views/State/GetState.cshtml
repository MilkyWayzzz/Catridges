@model Catridges.Domain.Entity.State

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<table class="table">
    <thead>
    <tr>
        <td>#</td>
        <td>Наименование</td>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td width="10">1</td>
        <td>@Model.Name</td>
        <td width="20"><a class=" btn btn-outline-info" asp-controller="State" asp-action="UpdateState" asp-route-id="@Model.Id">Изменить</a></td>
        <td width="20"><a class=" btn btn-outline-danger" asp-controller="State" asp-action="DeleteState" asp-route-id="@Model.Id">Удалить</a></td>
    </tr>
    </tbody>
</table>
<div style="position: center">
    <a class="btn btn-outline-info"  asp-controller="State" asp-action="GetAllStates">Назад</a>
</div>